<Group hide-input="false" hide-output="false" labelreference="L8" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#L5/1/C
restart;
with(PolynomialTools):
with(ArrayTools):
with(LinearAlgebra):
f := (x,y) -&gt; (-1)^k*(binomial(n,k))^3:
maxi := 2:
maxj := 2:
ex := 0:
params := {}:

for i from 0 to maxi do:
for j from 0 to maxj do:
 ex := ex + a(i,j)*f(n+j, k+i);
 params := {op(params), a(i,j)}:
end do:
end do:



num := numer(expand(ex / f(n,k) )):
vec := CoefficientVector(num, k):
L := {}:

for i from 1 to Dimension(vec) do:
L := {op(L), vec[i]=0};
end do:

solve(L, params);

solve({op(solve(L, params)), a(0,0)=1},params);
rsolve(x(n)-x(n+1)=0, x(n));
