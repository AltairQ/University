<Group hide-input="false" hide-output="false" labelreference="L5" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">#L5/1/B
restart;
with(PolynomialTools):
with(ArrayTools):
with(LinearAlgebra):
f := (x,y) -&gt; ((-1)^y)*(binomial(x,y))^2:
num := numer(expand((
f(n,k) + a*f(n+1,k) + b*f(n,k+1) + c*f(n+1,k+1) + d*f(n+2,k) + e*f(n+2,k+1) + g*f(n, k+2) + h*f(n+1,k+2) + j*f(n+2, k+2)
)/f(n,k))):
vec := CoefficientVector(num, k):
L := {}:
for i from 1 to Dimension(vec) do:
L := {op(L), vec[i]=0};
end do:
params := {a,b,c,d,e,g,h,j}:
solve(L, params);

simplify(rsolve(4*(n+1)*x(n) + (n+2)*x(n+2) = 0, x(n)), GAMMA);

</Text-field>
</Input>
