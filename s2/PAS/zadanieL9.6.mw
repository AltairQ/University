PetkFindN := proc(P, m, M, J)
	local l, gotsth, b, j, bsum, i, cmax, Ns;
	
	b := [];	
	
	for l from 0 to m do:
		b := [op(b), add(j^l *coeff(P[j+1], n, M-(m-l)+1), j=0..J )];			
	end do:


#	print(&quot;b is&quot;);
#	print(b);
	bsum := add(binomial(N,l)*b[l+1], l=0..m);
	Ns := [solve(bsum=0, N) assuming N::nonnegint];
#	print(&quot;internal solutions:&quot;);
#	print(Ns);

	gotsth := 0;

	for i in b do:
		if i &lt;&gt; 0 then gotsth := 1 fi
	end do:

	if gotsth = 0 then return -1 fi;

	cmax := -2;
	
	for i in Ns do:
		if type(i, nonnegint) then
			if i &gt; cmax then cmax := i fi
		fi		
	end do:

	return cmax;
		
	 
end proc:

GenEqsFromPoly := proc(p, n)
	local cof, eqlist;
	eqlist := [];
	
	for cof in PolynomialTools[CoefficientList](p, n) do:
		eqlist := [op(eqlist), cof=0];
	end do:

	return eqlist;
		
end proc:


PetkSolveEq := proc(equ, s, N)
	local body, subfunc, ts, t, sol, paramsol, nparams;
	body := n -&gt; (add(delta[N-i]*n^i, i=0..N));
	ts := eval(lhs(equ)-rhs(equ), s=body);
#	print(collect(ts,n));
	sol := solve(GenEqsFromPoly(ts, n), [seq(delta[N-i],i=0..N)]);
	paramsol:= collect(eval(body(n),sol[1]),n);

	nparams := [];
	
	for t in sol[1] do:
		if lhs(t) &lt;&gt; rhs(t) then
			nparams := [op(nparams), lhs(t)];
		fi
	end do:
#	print( nparams);

	return [paramsol, {seq(delta[N-i],i=0..N)} minus {op(nparams)}];
	

end proc:

#KOMENTARZ DO ROZWI\304\204ZANIA
#PetkovsekRecSolution przyjmuje dwa argumenty - pierwszy to r\303\263wnanie opisuj\304\205ce zwi\304\205zek liniowy jednorodny dowolnego rz\304\231du
#drugi to symbol stanowi\304\205cy niewiadom\304\205 funkcj\304\231 wielomianow\304\205 zmiennej 'n'.
#Wynikiem jest albo komunikat &quot;No polynomial solutions!&quot; albo lista dwuelementowa, kt\303\263rej pierwszy element to rozwi\304\205zanie og\303\263lne
#a drugi to zbi\303\263r parametr\303\263w wolnych. W moim odczuciu taki format pozwala na proste generowanie zar\303\263wno rozwi\304\205za\305\204 szczeg\303\263lnych
#jak i fundamentalnego uk\305\202adu rozwi\304\205za\305\204.
#Oczywi\305\233cie, &quot;no polynomial solutions&quot; oznacza brak nietrywialnych rozwi\304\205za\305\204, poniewa\305\274 wiemy \305\274e s=0 zawsze jest rozwi\304\205zaniem.

PetkovsekRecSolution := proc(equ, s)
	local r, m, shft, P, M, J, N;
#	konwersja r\303\263wnania na pojedyncze wyra\305\274enie
	r:=lhs(equ)-rhs(equ);

#	jako \305\274e Maple jest troch\304\231 kopni\304\231te to niech s(n+i) -&gt; S^i
	shft := k-&gt;S^(k-n);
	r:=eval(r, s=shft);
#	print(r);

	P := PolynomialTools[CoefficientList](r, S);
	J := degree(r, S);
	
#	print(P);
#	print(J);
	
	M := max(seq(degree(P[i+1], n), i=0..J ) );

	for m from 0 to J+1 do: #tutaj korzystam z gwarancji decyzji przed J krokami
		N := PetkFindN(P, m, M, J);

		if N = -2 then
			return &quot;No polynomial solutions!&quot;;
		fi;
		
		if N &gt;= 0 then:
#			print(N);
			return PetkSolveEq(equ, s, N); 
		fi;
	end do:	


	return &quot;end&quot;;
	
	


end proc:</EC-CodeEditor></CodeEditor-ExecGroup>
