	for x in permute(hyp[1]) do: #tutaj permutuj\304\231 parametry, \305\274eby potem si\304\231 nie przejmowa\304\207 kolejno\305\233ci\304\205
		for y in permute(hyp[2]) do:
			all_permuted := [op(all_permuted), [x,y,hyp[3]]];
